{
  "expo": {
    "name": "Qamqor",
    "slug": "mobile-social-assistance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7f1e6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meikuv.mobilesocialassistance"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "package": "com.meikuv.mobilesocialassistance"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [["expo-build-properties", { "android": { "usesCleartextTraffic": true } }]],
    "extra": {
      "eas": {
        "projectId": "faf7e1f5-480e-436e-b9cc-14a0d7f535c8"
      }
    }
  }
}
